
// this is a factorial function
def fac(x: Int): Int{
    // LOADREF 0
    // LOADI 1
    // EQUAL
    // JMP_IF_FALSE 2
    // LOADI 1
    // RETURN
    // LOADREF 0
    // LOADREF 0
    // LOADI 1
    // SUB
    // CALL fac
    // MUL
    // RETURN
    if (x == 1) {
        return 1
    }
    return x * fac(x - 1)
}


